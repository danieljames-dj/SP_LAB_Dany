COPY START 0
FIRST STL RETADR
 CLEAR X
 CLEAR A
 CLEAR S
 +LDT #4096
 TD =X'F1'
 JEQ *-3
 RD =X'F1'
 COMPR A,S
 JEQ *+11
 STCH BUFFER,X
 TIXR T
 JLT *-19
 STX LENGTH
 LDA LENGTH
 COMP #0
 CLEAR X
 CLEAR A
 CLEAR S
 +LDT #4096
 TD =X'F2'
 JEQ *-3
 RD =X'F2'
 COMPR A,S
 JEQ *+11
 STCH RETADR,X
 TIXR T
 JLT *-19
 STX THREE
EOF BYTE C'EOF'
THREE WORD 3
RETADR RESW 1
BUFFER RESB 4096
